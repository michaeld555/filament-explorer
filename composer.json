{
    "name": "michaeld555/filament-explorer",
    "type": "library",
    "description": "A powerful file, folder, and media browser with an integrated code editor for Laravel Filament.",
    "keywords": [
        "php",
        "laravel",
        "filament",
        "file-explorer",
        "media-browser",
        "code-editor",
        "file-manager"
    ],
    "homepage": "https://github.com/michaeld555/filament-explorer",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Michaeld555\\FilamentExplorer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Michaeld555\\FilamentExplorer\\FilamentExplorerServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Michael Douglas",
            "email": "usermich999@gmail.com"
        },
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1",
        "tomatophp/console-helpers": "^1.1",
        "filament/filament": "^3.0",
        "maatwebsite/excel": "^3.1",
        "creagia/filament-code-field": "^3.0",
        "mallardduck/blade-boxicons": "^2.4",
        "calebporzio/sushi": "^2.5"
    }
}
